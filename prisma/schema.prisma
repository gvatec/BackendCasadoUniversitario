// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = ""
}

model Users {
  id Int @id @default(autoincrement())
  email String  
  name String
  password String
  datanascimento DateTime
  telefone Int

  @@map("users")
}
