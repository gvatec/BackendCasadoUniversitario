generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://admin:admin@localhost:3306/db_casadouniversitario"
}

model users {
  id             String    @id @unique @default(uuid())
  name           String
  email          String
  telefone       String
  datanascimento String
  password       String
  cpf            Int?
  nomemae        String?
  rg             String?
  urlfoto        String?
  created_at     DateTime? @default(now())
  updated_at     DateTime? @default(now())

  @@map("users")
}

model session {
  iduser         String    @id @unique
  name           String
  email          String
  telefone       String
  datanascimento String
  created_at     DateTime? @default(now())
  updated_at     DateTime? @default(now())

  @@map("session")
}
