generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:NJsd2m6wLhbv65e2xRjh@containers-us-west-160.railway.app:6623/railway"
}

model users {
  id             String    @id @unique @default(uuid())
  name           String
  email          String
  telefone       String
  datanascimento String
  password       String
  cpf            String?
  nomemae        String?
  rg             String?
  urlfoto        String?
  cep            String?
  created_at     DateTime? @default(now())
  updated_at     DateTime? @default(now())

  @@map("users")
}

model session {
  iduser         String    @id @unique
  name           String
  email          String
  telefone       String
  datanascimento String
  urlfoto        String?
  created_at     DateTime? @default(now())
  updated_at     DateTime? @default(now())

  @@map("session")
}
